{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-debugbar": "^2.4",
        "cfrutos/flaubert": "dev-master",
        "doctrine/orm": "^2.5",
        "gedmo/doctrine-extensions": "^2.4",
        "gloudemans/shoppingcart": "^2.3",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravel-doctrine/extensions": "1.0.*",
        "laravel-doctrine/migrations": "1.0.*",
        "laravel-doctrine/orm": "1.3.*",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "moneyphp/money": "^3.0",
        "myclabs/php-enum": "^1.5",
        "mockery/mockery": "^0.9.9",
        "paypal/rest-api-sdk-php": "*",
        "php-di/php-di": "^5.4",
        "predis/predis": "^1.0",
        "symfony/intl": "^4.0",
        "vlucas/phpdotenv": "^2.4",
        "yajra/laravel-datatables-oracle": "^7.5",
        "pda/pheanstalk": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "fzaninotto/faker": "^1.6",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/migrations": "^1.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "EventoOriginal\\Core\\": "../../core/src"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/money.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "app/",
            "EventoOriginal\\Core\\": "../../core/src"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "name": "cfrutos/flaubert",
            "type": "git",
            "url": "git@github.com:cfrutos/flaubert.git"
        }
    ]
}
